{
 "display_name": "Python 3 (Spark 2.2.1)",
 "language": "python",
 "argv": [
  "python3",
  "-m",
  "ipykernel",
  "-f",
  "{connection_file}"
 ],
 "env": {
  "SPARK_HOME": "/usr/local/spark-2.2.1-bin-hadoop2.7",
  "PYTHONPATH": "/usr/local/spark-2.2.1-bin-hadoop2.7/python/:/usr/local/spark-2.2.1-bin-hadoop2.7/python/lib/py4j-0.10.4-src.zip",
  "PYTHONSTARTUP": "/usr/local/spark-2.2.1-bin-hadoop2.7/python/pyspark/shell.py",
  "PYSPARK_PYTHON": "/usr/bin/python3",
  "PYSPARK_SUBMIT_ARGS": "--master local[2] pyspark-shell",
  "SPARK_DRIVER_MEMORY":"2G",
  "SPARK_LOCAL_IP":"127.0.0.1"
 }
}
